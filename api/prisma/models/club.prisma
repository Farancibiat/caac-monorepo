model Club {
  id        Int      @id @default(autoincrement())
  nombre    String   @unique
  isActive  Boolean  @default(true)
  createdAt DateTime @default(now()) @map("created_at")
  updatedAt DateTime @updatedAt @map("updated_at")
  
  // Relaci√≥n con Profile
  profiles  Profile[]

  @@map("clubs")
  @@schema("public")
} 